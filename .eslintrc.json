{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "no-console": 0,
        "max-len": [1, 120, 4],
        "no-param-reassign": 0,
        "consistent-return": 0,
        "radix": 0,
        "func-names": 0,
        "no-underscore-dangle": 0,
        "camelcase": "off",
        "no-plusplus": 0,
        "class-methods-use-this": 0
    }
}